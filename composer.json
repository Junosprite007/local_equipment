{
	"name": "local/equipment",
	"type": "moodle-local",
	"require": {
		"php": ">=8.1.0",
		"aws/aws-sdk-php": "^3.297.1",
		"guzzlehttp/guzzle": "^7.8.1",
		"guzzlehttp/promises": "^2.0",
		"guzzlehttp/psr7": "^2.6.2"
	},
	"config": {
		"vendor-dir": "vendor",
		"platform": {
			"php": "8.4.3"
		},
		"allow-plugins": {
			"aws/aws-sdk-php": true
		},
		"optimize-autoloader": true
	}
}
